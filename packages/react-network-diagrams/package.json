{
  "name": "react-network-diagrams",
  "version": "1.0.0",
  "description": "Declarative timeseries charts",
  "keywords": [
    "network",
    "circuits",
    "maps",
    "concatenated",
    "parallel",
    "editor",
    "react"
  ],
  "main": "lib/index",
  "homepage": "http://software.es.net/react-network-diagrams",
  "author": "ESnet Tools Team <tools@es.net>",
  "repository": "esnet/react-network-diagrams",
  "bugs": {
    "url": "https://github.com/esnet/react-network-diagrams/issues"
  },
  "scripts": {
    "docs": "echo \"*** Building API docs\n\" && react-docgen src/components/*.js -x js -o ../website/src/api/docs.json --pretty",
    "lint": "eslint src/**/*.js",
    "test": "npm run lint",
    "watch": "echo \"*** Watching...\n\" && babel src/components --optional runtime --stage 0 --out-dir lib/components  --watch",
    "build": "echo \"*** Building lib\n\" && rm -rf lib && mkdir lib && mkdir lib/components && cp src/components/*.css lib/components && babel src/components --optional runtime --stage 0 --out-dir lib/components && babel src/index.js --optional runtime --stage 0 --out-file lib/index.js",
    "start-website": "npm run docs && webpack-dev-server --config webpack.config.js",
    "build-website": "webpack --config webpack.config.js"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "license": "BSD-3-Clause-LBNL",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "d3-scale": "^0.7.2",
    "keymirror": "^0.1.1",
    "underscore": "^1.7.0",
    "victor": "^0.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundle-loader": "^0.5.4",
    "create-react-class": "^15.6.2",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "lerna": "^2.4.0",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-select": "^1.0.0-rc.10",
    "react-docgen": "^2.19.0",
    "react-scripts": "^1.0.14",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
